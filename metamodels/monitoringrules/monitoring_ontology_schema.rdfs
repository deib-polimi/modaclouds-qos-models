<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:mo="http://www.modaclouds.eu/rdfs/1.0/monitoring#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Service">
    <mo:requires rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Service"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Component">
    <mo:provides>
      <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Method"/>
    </mo:provides>
    <rdfs:subClassOf rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Service"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Location"/>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Region">
    <rdfs:subClassOf rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Location"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Method">
    <mo:calls rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Method"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Provider">
    <mo:offers rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Service"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#VM">
    <rdfs:subClassOf rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Service"/>
    <mo:isIn rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Location"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.modaclouds.eu/rdfs/1.0/monitoring#Zone">
    <mo:isIn rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Region"/>
    <rdfs:subClassOf rdf:resource="http://www.modaclouds.eu/rdfs/1.0/monitoring#Location"/>
  </rdfs:Class>
</rdf:RDF>