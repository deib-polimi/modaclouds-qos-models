@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mo:      <http://www.modaclouds.eu/rdfs/1.0/monitoring/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

mo:VM
      a       rdfs:Class ;
      rdfs:subClassOf mo:Service ;
      mo:isIn mo:Location .

mo:Zone
      a       rdfs:Class ;
      rdfs:subClassOf mo:Location ;
      mo:isIn mo:Region .

mo:Service
      a       rdfs:Class ;
      mo:requires mo:Service .

mo:Provider
      a       rdfs:Class ;
      mo:offers mo:Service .

mo:Location
      a       rdfs:Class .

mo:Region
      a       rdfs:Class ;
      rdfs:subClassOf mo:Location .

mo:Method
      a       rdfs:Class ;
      mo:calls mo:Method .

mo:Component
      a       rdfs:Class ;
      rdfs:subClassOf mo:Service ;
      mo:provides mo:Method .
