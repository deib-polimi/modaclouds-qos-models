@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mo:      <http://www.modaclouds.eu/rdfs/1.0/monitoring/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

mo:Frontend
      a       rdfs:Class ;
      rdfs:subClassOf mo:InternalComponent ;
      mo:provides mo:Login ;
      mo:requires mo:FrontendServer .

mo:VM
      a       rdfs:Class .

mo:Login
      a       rdfs:Class ;
      rdfs:subClassOf mo:Method .

mo:FrontendServer
      a       rdfs:Class ;
      rdfs:subClassOf mo:VM .

mo:InternalComponent
      a       rdfs:Class .

mo:Method
      a       rdfs:Class .